import com.redhat.gpte.util.DroolsCommandHelper

global ArrayList accreditationList



rule "4_Red Hat Delivery Specialist - Advanced Cloud Infrastructure_with_6_courses"
when
    $student:Student()
    $cc1:CourseCompletion(courseName == "OpenStack 7 FASTRAX")
    $cc2:CourseCompletion(this != $cc1, courseName == "OpenStack 7 Implementation")
    $cc3:CourseCompletion(this != $cc1, this != $cc2, courseName == "Red Hat Ceph Storage for OpenStack")
    $cc4:CourseCompletion(this != $cc1, this != $cc2, this != $cc3, courseName == "OpenStack Director")
    $cc5:CourseCompletion(this != $cc1, this != $cc2, this != $cc3, this != $cc4, courseName == "OpenStack Advanced Storage")
    $cc6:CourseCompletion(this != $cc1, this != $cc2, this != $cc3, this != $cc4, this != $cc5, courseName == "OpenStack Advanced Networking")
    eval(DroolsCommandHelper.isLatestCourseCompletionWithinDates(false, "1-Mar-2016", "1-Jul-2016", $cc1, $cc2, $cc3, $cc4, $cc5, $cc6 ))
then
    CourseCompletion latestCC = DroolsCommandHelper.determineMostRecentCourseCompletion(false, $cc1, $cc2, $cc3, $cc4, $cc5, $cc6 );

    Accreditation accreditation = new Accreditation();
    accreditation.setAccreditationName("Red Hat Delivery Specialist - Advanced Cloud Infrastructure");
    accreditation.setStudentId($student.getStudentid());
    accreditation.setEmail($student.getEmail());
    accreditation.setCourseId(latestCC.getCourseId());
    accreditation.setCourseName(latestCC.getCourseName());
    accreditation.setAccreditationDate(latestCC.getAssessmentDate());
    accreditation.setAccreditationType("Active");
    accreditation.setRuleFired(drools.getRule().getName());

    accreditationList.add(accreditation);
    System.out.println("   [ "+ $student.getEmail()+" : Fired: " + drools.getRule().getName() + " ]");
end

